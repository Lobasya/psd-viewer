*
	box-sizing: border-box

html
	font-family: sans-serif
	font-variant-ligatures: common-ligatures
	vendor(font-feature-settings, "kern")
	line-height: 1.3
	font-size: 18px

html
body
	margin: 0
	padding: 0

img
	display: block
	max-width: 100%

@keyframes rotate {
	from { transform: rotate(0deg) }
	to { transform: rotate(359deg) }
}
